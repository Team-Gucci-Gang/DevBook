<script src="https://unpkg.com/darkreader"></script>
<script>
  if (!localStorage.nightMode) {
    localStorage.nightMode = "off";
  } else {
    toggleNight(true);
  }
  $("#darkmode").on("click", toggleNight);
  function toggleNight(force) {
    console.log(localStorage.nightMode);
    if (localStorage.nightMode == "on" && force != true) {
      console.log("Disable");
      DarkReader.disable();
      localStorage.nightMode = "off";
    } else {
      console.log("Enable");
      DarkReader.enable();
      localStorage.nightMode = "on";
    }
  }
</script>
